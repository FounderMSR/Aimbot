--// get sub prefix or smth lmfao
function GetSubPrefix(str)
    local str = tostring(str):gsub(" ", "")
    local var = ""

    if #str == 2 then
        local sec = string.sub(str, #str, #str + 1)
        var = sec == "1" and "st" or sec == "2" and "nd" or sec == "3" and "rd" or "th"
    end

    return var
end


